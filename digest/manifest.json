{
    "title": "Mega-Paysage TXT",
    "programName": "Digest",
    "packageName": "digest",
    "isDefault": false,
    "requiresDatabaseAccess": true,
    "dataHeaders": [
        "Unidade",
        "N. Contrato",
        "CPF/CNPJ",
        "Nome",
        "Quadra"
    ],
    "requirements": [
        "xlrd==1.2.0",
        "pandas==0.25.3"
    ],
    "allowedOutputExtensions": [
        {
            "name": "Text",
            "extensions": [
                "txt"
            ]
        }
    ],
    "defaultSettings": {
        "network": {
            "title": "Rede",
            "entries": {
                "ip": {
                    "title": "IP Address",
                    "setting": "0.0.0.0",
                    "type": "ipAddress"
                },
                "port": {
                    "title": "Port",
                    "setting": "5000",
                    "type": "port"
                }
            }
        },
        "credentials": {
            "title": "Autentificação",
            "entries": {
                "username": {
                    "title": "Usuário",
                    "setting": "guest",
                    "type": "username"
                },
                "password": {
                    "title": "Senha",
                    "setting": "fawkes",
                    "type": "password"
                }
            }
        }
    },
    "form": [
        {
            "id": "planilhas-saldo",
            "label": "Planilhas Saldo",
            "type": "file-input",
            "allowedExtensions": [
                {
                    "name": "Excel",
                    "extensions": [
                        "xlsx",
                        "xls"
                    ]
                }
            ]
        }
    ]
}
